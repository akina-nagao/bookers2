<h2>Users</h2>
  <table class="table">
    <thead>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= image_tag user.get_profile_image(100,100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "show", user_path(user.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>